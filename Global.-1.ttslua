	end
	if update_version then
		msg = msg .. '\nupdate_version = ' .. update_version
	else
		msg = msg .. '\nupdate_version = nil'
	end
	msg = msg .. '\nGlobal Lua length ' .. string.len(Global.getLuaScript())

	return msg
end

